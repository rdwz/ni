    "find-up": "^6.3.0",
    "fs-extra": "^11.2.0",
    "fzf": "^0.5.2",
    "ini": "^4.1.1",
    "kleur": "^4.1.5",
    "rimraf": "^5.0.5",
    "terminal-link": "^3.0.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6",
    "which": "^4.0.0"
  }
