{
  "name": "@redwerkz/ni",
  "type": "module",
  "version": "0.21.14",
  "packageManager": "pnpm@8.11.0",
  "description": "Use the right package manager",
  "author": "Marc Redwerkz",
  "license": "MIT",
  "homepage": "https://github.com/rdwz/ni#readme",
  "bugs": {
    "url": "https://github.com/rdwz/ni/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "na": "bin/na.mjs",
    "nci": "bin/nci.mjs",
    "ni": "bin/ni.mjs",
    "nlx": "bin/nlx.mjs",
    "nr": "bin/nr.mjs",
    "nu": "bin/nu.mjs",
    "nun": "bin/nun.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "esno src/commands/ni.ts",
    "lint": "eslint .",
    "nr": "esno src/commands/nr.ts",
